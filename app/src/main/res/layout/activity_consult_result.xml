<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/marginApp"
    tools:context=".ConsultResultActivity">

    <TextView
        android:id="@+id/tv_Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tvConsultResult"
        android:textColor="@color/black"
        android:textSize="@dimen/tv_size_title"
        android:textStyle="bold"
        />
    <LinearLayout
        android:id="@+id/ll_SelectTeam1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/margin_between_tv_and_et"
        >

        <EditText
            android:id="@+id/edit_SelectTeam1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:enabled="false"
            android:hint="@string/hint_edit_Team1"
            android:textSize="@dimen/edit_size_hint" />

        <Button
            android:id="@+id/btn_SelectTeam1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/txt_btn_SelectTeam"
            android:backgroundTint="@color/colors_btn"
            />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_ContainerFrame"
        android:layout_width="@dimen/width_ll"
        android:layout_height="@dimen/height_ll"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/margin_start_ll"
        android:layout_marginTop="@dimen/margin_top_ll"
        android:orientation="vertical">

        <FrameLayout
            android:id="@+id/fl_Result1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_frame">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/fl_Result2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_frame">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/fl_Result3"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_frame">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/fl_Result4"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_frame"
            >

        </FrameLayout>

        <FrameLayout
            android:id="@+id/fl_Result5"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_frame">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/fl_Result6"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_frame">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/fl_Result7"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_frame">

        </FrameLayout>
    </LinearLayout>


</RelativeLayout>